
include_rules

include $(COMPILE_TUP)

: foreach *.cpp |> !cxx |>

include $(LINK_TUP)

: ./generate.o |> !linker |> generate

: ./naive.o |> !linker |> naive
: ./array.o |> !linker |> array

LIBS += -lpthread

: ./msd.o |> !linker |> msd
: ./msd.const_char_star.o |> !linker |> msd.const_char_star
